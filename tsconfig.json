{
  "compilerOptions": {
    "target": "es2016",
    "lib": [],
    "module": "commonjs",
    // "baseUrl": "./",
    // "paths": {},
    // "typeRoots": [],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
