{
"extends": "../../tsconfig.json",
"compilerOptions": {
    "lib": [
    "DOM",
    "ES2020"
    ],
    "outDir": "./dist",
    "baseUrl": ".",
    "declaration": false,
    "paths": {
    "@/*": [
        "./src/*"
    ],
    "*": [
        "./typings/*"
    ]
    },
    "typeRoots": [
    "./node_modules/@types",
    "../../node_modules/@types",
    ],
},
"include": [
    "./typings/**/*",
    "./src/**/*"
]
}
